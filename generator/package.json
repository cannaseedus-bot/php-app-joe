{
  "name": "citibiz-generator",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n UI,API -c cyan,magenta \"npm --prefix ui run dev\" \"npm --prefix server run dev\"",
    "build-ui": "npm --prefix ui run build",
    "serve-ui": "npm --prefix ui run preview",
    "dev-api": "npm --prefix server run dev",
    "build": "npm run build-ui"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
